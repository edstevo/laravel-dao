{
    "name": "edstevo/laravel-dao",
    "description": "Laravel Dao Package with built in caching of data objects, validation and model generation",
    "keywords": ["laravel", "dao", "cache"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "edstevo",
            "email": "edward.stephenson@me.com"
        }
    ],
    "require": {
        "laravel/framework": "5.6.*"
    },
    "autoload": {

        "psr-4": {
            "EdStevo\\Dao\\": "src/"
        }
    },
    "autoload-dev": {

        "psr-4": {
            "EdStevo\\Dao\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdStevo\\Dao\\DaoServiceProvider"
            ]
        }
    }
}
